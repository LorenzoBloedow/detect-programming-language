{
  "name": "detect-programming-language",
  "version": "1.0.3",
  "description": "Detects programming languages by file extension insanely fast.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/LorenzoBloedow/detect-programming-language"
  },
  "scripts": {
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:types": "tsc --project tsconfig.types.json",
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "postbuild": "renamer --find js --replace mjs dist/esm/* && renamer --find js --replace cjs dist/cjs/*",
    "test": "jest"
  },
  "keywords": [
    "Programming",
    "Language",
    "Detect",
    "File",
    "Extension"
  ],
  "files": [
    "/dist/types",
    "/dist/cjs",
    "/dist/esm"
  ],
  "author": "LorenzoBloedow",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "^7.22.5",
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^29.2.2",
    "babel-jest": "^29.2.2",
    "jest": "^29.2.2",
    "jsmin": "^1.0.1",
    "renamer": "^4.0.0",
    "terser": "^5.19.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=17.1.0"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": "./dist/cjs/index.cjs",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs",
      "default": "./dist/esm/index.mjs"
    }
  }
}
